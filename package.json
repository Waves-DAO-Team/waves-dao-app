{
  "name": "dappgrants",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "run-s start:*",
    "start:env": "envsub --env-file .env ./projects/dapp/src/environments/environment.ts ./projects/dapp/src/environments/environment.local.ts",
    "start:serve": "ng serve",
    "build": "ng build --configuration=production",
    "test": "ng test",
    "tests": "yarn start:env && run-s test:*",
    "test:dapp": "ng test dapp --browsers=ChromeHeadless --watch=false --progress=false",
    "test:services": "ng test services --browsers=ChromeHeadless --watch=false --progress=false",
    "test:ui": "ng test ui --browsers=ChromeHeadless --watch=false --progress=false",
    "lint": "run-s lint:es",
    "lint:es": "ng lint",
    "e2e": "ng e2e",
    "lint:code": "ts-standard",
    "commit": "lint-staged --allow-empty",
    "push": "run-s lint:es build",
    "postinstall": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.3",
    "@angular/cdk": "^12.0.3",
    "@angular/common": "^12.0.3",
    "@angular/compiler": "^12.0.3",
    "@angular/core": "^12.0.3",
    "@angular/forms": "^12.0.3",
    "@angular/material": "^12.0.3",
    "@angular/platform-browser": "^12.0.3",
    "@angular/platform-browser-dynamic": "^12.0.3",
    "@angular/router": "^12.0.3",
    "@ngneat/transloco": "^2.21.0",
    "@ngxd/core": "^11.0.1",
    "@types/lodash": "^4.14.170",
    "@waves.exchange/provider-web": "^1.0.2",
    "@waves/signer": "^1.0.2",
    "angular-gtag": "^1.0.4",
    "envsub": "^4.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-unused-imports": "^1.1.1",
    "include-media": "^1.4.10",
    "ngx-markdown": "^12.0.1",
    "ngx-mask": "^12.0.0",
    "ngx-progressbar": "^6.1.1",
    "npm-run-all": "^4.1.5",
    "pm2": "^4.5.6",
    "prettier": "^2.3.0",
    "puppeteer": "^10.0.0",
    "rxjs": "^7.1.0",
    "tslib": "^2.2.0",
    "tslint-etc": "^1.13.10",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.3",
    "@angular-eslint/builder": "^12.1.0",
    "@angular-eslint/eslint-plugin": "^12.1.0",
    "@angular-eslint/eslint-plugin-template": "^12.1.0",
    "@angular-eslint/schematics": "^12.1.0",
    "@angular-eslint/template-parser": "^12.1.0",
    "@angular/cli": "^12.0.3",
    "@angular/compiler-cli": "^12.0.3",
    "@types/jasmine": "^3.7.6",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^15.12.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^6.0.0",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "lint-staged": "^11.0.0",
    "markdownlint-cli": "^0.27.1",
    "ng-packagr": "^12.0.3",
    "protractor": "^7.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "ts-node": "^10.0.0",
    "typescript": "4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn commit",
      "pre-push": "yarn push"
    }
  },
  "ts-standard": {
    "globals": [
      "jasmine"
    ],
    "ignore": [
      "**/*.spec.ts"
    ]
  },
  "sideEffects": false
}
