{
  "name": "dappgrants",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "run-s start:*",
    "start:env": "envsub --env-file .env ./projects/dapp/src/environments/environment.ts ./projects/dapp/src/environments/environment.local.ts",
    "start:serve": "ng serve",
    "build": "run-s build:*",
    "build:env": "envsub --env-file .env ./projects/dapp/src/environments/environment.ts ./projects/dapp/src/environments/environment.prod.ts",
    "build:build": "ng build --prod",
    "test": "ng test",
    "tests": "yarn start:env && run-p  test:*",
    "test:dapp": "ng test dapp --browsers=ChromeHeadless --watch=false --progress=false",
    "test:services": "ng test services --browsers=ChromeHeadless --watch=false --progress=false",
    "test:ui": "ng test ui --browsers=ChromeHeadless --watch=false --progress=false",
    "lint": "run-s lint:es",
    "lint:es": "ng lint",
    "e2e": "ng e2e --port 0",
    "lint:code": "ts-standard",
    "commit": "lint-staged --allow-empty",
    "push": "run-s lint:es build",
    "envsub": "envsub --diff ./projects/dapp/src/environments/environment.prod.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.3",
    "@angular/cdk": "^11.2.2",
    "@angular/common": "~11.2.3",
    "@angular/compiler": "~11.2.3",
    "@angular/core": "~11.2.3",
    "@angular/forms": "~11.2.3",
    "@angular/material": "^11.2.2",
    "@angular/platform-browser": "~11.2.3",
    "@angular/platform-browser-dynamic": "~11.2.3",
    "@angular/router": "~11.2.3",
    "@ngneat/transloco": "^2.19.1",
    "@ngxd/core": "^10.0.0",
    "@types/lodash": "^4.14.168",
    "@waves.exchange/provider-web": "^0.2.5",
    "@waves/signer": "0.1.2",
    "envsub": "^4.0.7",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "include-media": "^1.4.9",
    "ngx-markdown": "^10.1.1",
    "ngx-mask": "^11.0.1",
    "ngx-progressbar": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "pm2": "^4.5.4",
    "prettier": "^2.2.1",
    "rxjs": "~6.6.6",
    "tslib": "^2.1.0",
    "tslint-etc": "^1.13.9",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.2",
    "@angular-eslint/builder": "1.0.0",
    "@angular-eslint/eslint-plugin": "1.0.0",
    "@angular-eslint/eslint-plugin-template": "1.0.0",
    "@angular-eslint/schematics": "1.0.0",
    "@angular-eslint/template-parser": "1.0.0",
    "@angular/cli": "~11.2.2",
    "@angular/compiler-cli": "~11.2.3",
    "@types/jasmine": "3.6.4",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "4.3.0",
    "eslint": "^7.18.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^4.3.8",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.2.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "lint-staged": "^10.4.2",
    "markdownlint-cli": "^0.24.0",
    "ng-packagr": "^11.1.2",
    "protractor": "^7.0.0",
    "standardx": "^5.0.0",
    "stylelint": "^13.9.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn commit",
      "pre-push": "yarn push"
    }
  },
  "ts-standard": {
    "globals": [
      "jasmine"
    ],
    "ignore": [
      "**/*.spec.ts"
    ]
  },
  "sideEffects": false
}
